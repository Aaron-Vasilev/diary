package pages

import "github.com/aaron-vasilev/diary-templ/src/components"

templ Login() {
  @components.Layout() {
    <div class="flex w-screen justify-center">
      <div class="flex flex-col gap-2 border-sky-400">
        <a href="http://localhost:3000/auth/login?provider=google">
          Login with GOOGLE
        </a>
        <a href="http://localhost:3000/diary">
          or continue without login 
        </a>
      </div>
    </div>
  }
}
